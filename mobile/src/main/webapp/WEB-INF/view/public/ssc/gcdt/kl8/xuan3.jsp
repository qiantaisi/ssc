<script>
    var playGroupId = 8;
    var playId = 273;
    var playGroupName = '选3';

    function getZhudan(inputMoney, number) {
        if ($(".cl-604 table tbody tr td.active").length != 3) {
            Tools.alert("请选择3个号码");
            return;
        }

        var tmp = [];
        $(".cl-604 table tbody tr td.active").each(function () {
            tmp.push($(this).data("name"));
        });

        var betForm = {
            totalMoney: 0,
            sscBetList: [],
            totalZhushu: 0
        };

            betForm.sscBetList.push({
                playGroupName: playGroupName,
                playGroupId: playGroupId,
                number: number,
                playId: playId,
                zhushu: 1,
                perMoney: inputMoney,
                content: tmp.join(","),
                playPlId: $("#pl3").data('plid'),
                playPl: $("#pl3").data("pl")
            });
            betForm.totalMoney = add(betForm.totalMoney, inputMoney);
            betForm.totalZhushu = add(betForm.totalZhushu, 1);
        return betForm;
    }
    function getZhushu() {
        if ($(".cl-604 table tbody tr td.active").length != 3) {
            return 0;
        }
        return 1;
    }
</script>